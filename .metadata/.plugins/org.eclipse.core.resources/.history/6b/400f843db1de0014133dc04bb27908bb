package com.pramati.imaginea.base;

/**This is an generic Interface to represent crawling,
 * There can be different types of crawlers but the behaiour will
 * be same.
 * @author anandu
 *
 */
public interface Crawlar {
	

	/** 
	 * @throws Exception
	 */
	public void crawl () throws Exception;
	public void addPage(Page targetPage) throws Exception;
	public void shutDown() throws Exception;
}
