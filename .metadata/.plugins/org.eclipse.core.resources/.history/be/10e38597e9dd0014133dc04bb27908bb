package com.pramati.imaginea.base;

/**
 * @author anandu
 *
 */
public interface Page {

	/**
	 * @param url
	 * @return
	 * @throws Exception
	 */
	public Object load(Url url) throws Exception;

	/**
	 * @param url
	 * @return
	 * @throws Exception
	 */
	public void save() throws Exception;
}
