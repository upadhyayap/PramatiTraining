/**
 * 
 */
package com.pramati.imaginea.startup;

import com.pramati.imaginea.bObj.HttpWebpage;
import com.pramati.imaginea.base.crawlable;
import com.pramati.imaginea.utilities.CrawlerConstants;

/** Starting point of the Web Crawler Package 
 * @author anandu
 *
 */
public class Start {
	
	/**
	 * URL of the web page used to crawl the page further.This URL represents a
	 * particular web page. This is immutable field once a Web page class is
	 * instantiated the furher the URL can not be changed
	 */
	
	private static crawlable gWebpage;
	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		try {
			gWebpage = new HttpWebpage(CrawlerConstants.gRootUrl);
			gWebpage.crawl();
		} catch (Throwable e) {
			System.out.println("Error");
		}
	}

}
