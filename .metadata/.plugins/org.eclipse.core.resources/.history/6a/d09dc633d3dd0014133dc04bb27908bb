 * 
 */
package com.pramati.imaginea.base;

/**
 * @author anandu
 *
 */
public abstract class pageContent {
	
	protected String webContent;

public MailContent() {
		
	}
	public pageContent(String content) {
		this.webContent = content;
	}
	
	/**
	 * @return the webContent
	 */
	public String getWebContent() {
		return webContent;
	}
}
