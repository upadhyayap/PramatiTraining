package com.pramati.imaginea.base;

/**
 * @author anandu
 *
 */
public interface Page {
	
	/**
	 * @param url
	 * @return
	 * @throws Exception
	 */
	public Object load  (Object url) throws Exception;
	/**
	 * @param url
	 * @return
	 * @throws Exception
	 */
	public Object parse (Object url) throws Exception;
	/**
	 * @param url
	 * @return
	 * @throws Exception
	 */
	public Object save  (Object url) throws Exception;
}
