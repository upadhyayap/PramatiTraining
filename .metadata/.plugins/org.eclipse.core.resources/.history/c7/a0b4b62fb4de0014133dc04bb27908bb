package com.pramati.imaginea.base;

import java.io.File;

/**This Interface represents a web page element it can be a web text 
 * or web image or web link. This is a generic representation of page 
 * elements.
 * @author anandu
 *
 */
public interface WebElement {
	
	public void load() throws Exception;
	public void save(File targetFile) throws Exception;
}
